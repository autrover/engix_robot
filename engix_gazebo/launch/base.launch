<?xml version="1.0"?>

<launch>
    <!-- Differential controller parameters and basic localization -->
    <include file="$(find engix_control)/launch/control.launch" />

    <!--Teleop -->
    <include file="$(find engix_control)/launch/teleop.launch" />

    <!-- Diagnostic Aggregator -->
    <node pkg="diagnostic_aggregator" type="aggregator_node" name="diagnostic_aggregator">
        <rosparam command="load" file="$(find engix_control)/config/diagnostics.yaml"/>
    </node>

</launch>
