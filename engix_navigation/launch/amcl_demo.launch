<?xml version="1.0"?>

<launch>

  <!-- Run the map server -->
  <arg name="map_file" default="$(find engix_navigation)/maps/playpen_map.yaml"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />

  <!--- Run AMCL -->
  <include file="$(find engix_navigation)/launch/amcl1.launch" />

  <!--- Run Move Base -->
  <include file="$(find engix_robot)/launch/move_base.launch" />

</launch>
